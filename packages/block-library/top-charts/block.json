{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/wp/6.8/schemas/json/block.json",
	"apiVersion": 3,
	"name": "profile-blocks-lastfm/top-charts",
	"title": "LastFM Top Charts",
	"textdomain": "profile-blocks-lastfm",
	"attributes": {
		"username": {
			"type": "string",
			"default": ""
		},
		"collection": {
			"type": "string",
			"default": "albums",
			"enum": [
				"albums",
				"artists",
				"tracks"
			]
		},
		"itemsToShow": {
			"type": "number",
			"default": 6
		}
	},
	"supports": {
		"html": false,
		"spacing": {
			"padding": true,
			"margin": true,
			"blockGap": true
		},
		"layout": {
			"allowSizingOnChildren": true,
			"allowSwitching": true
		},
		"__experimentalBorder": {
			"radius": true,
			"color": true,
			"width": true,
			"style": true
		}
	},
	"editorScript": "file:index.js",
	"editorStyle": "file:index.css",
	"style": "file:style-index.css"
}