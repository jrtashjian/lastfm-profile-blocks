{
    "$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/wp/6.8/schemas/json/block.json",
    "apiVersion": 3,
    "name": "profile-blocks-lastfm/item-name",
    "title": "Item Name",
    "textdomain": "profile-blocks-lastfm",
    "ancestor": [
        "profile-blocks-lastfm/dynamic-template"
    ],
    "attributes": {
        "itemTextProp": {
            "type": "string"
        },
        "itemLinkProp": {
            "type": "string"
        },
        "isLink": {
            "type": "boolean",
            "default": false
        },
        "linkTarget": {
            "type": "string",
            "default": "_blank"
        }
    },
    "usesContext": [
        "item"
    ],
    "editorScript": "file:index.js",
    "editorStyle": "file:index.css"
}