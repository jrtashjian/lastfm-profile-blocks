{
    "$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/wp/6.8/schemas/json/block.json",
    "apiVersion": 3,
    "name": "lastfm-profile-blocks/item-image",
    "title": "Item Image",
    "textdomain": "lastfm-profile-blocks",
    "ancestor": [
        "lastfm-profile-blocks/dynamic-template"
    ],
    "attributes": {
        "itemImageProp": {
            "type": "string"
        },
        "itemLinkProp": {
            "type": "string"
        },
        "width": {
            "type": "number"
        },
        "isLink": {
            "type": "boolean",
            "default": false
        },
        "linkTarget": {
            "type": "string",
            "default": "_blank"
        }
    },
    "supports": {
        "color": {
            "text": false,
            "background": false
        },
        "spacing": {
            "margin": true,
            "padding": true,
            "__experimentalDefaultControls": {
                "margin": false,
                "padding": false
            }
        },
        "filter": {
            "duotone": true
        }
    },
    "selectors": {
        "filter": {
            "duotone": ".wp-block-lastfm-profile-blocks-item-image img"
        }
    },
    "usesContext": [
        "item"
    ],
    "editorScript": "file:index.js",
    "editorStyle": "file:index.css",
    "style": "file:style-index.css"
}